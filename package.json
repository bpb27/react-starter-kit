{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React-Node app ready to go",
  "engines": {
    "node": "14.x"
  },
  "main": "index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "dev": "webpack-dev-server --mode development",
    "format": "prettier src/**/*.js --write --config ./.prettierrc.js",
    "generate": "node ./scripts/generate-component",
    "start": "node ./server",
    "test": "jest ./src"
  },
  "author": "Brendan Brown",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^2.0.2",
    "compression-webpack-plugin": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.21.5",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-prop-type-error": "^1.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "node-fetch": "^3.1.1",
    "prettier": "^2.2.1",
    "readline": "^1.3.0",
    "tar": "^6.0.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "jest": {
    "modulePaths": [
      "src",
      "test-setup"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/test-setup/style-mock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test-setup/file-mock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup/setup.js",
      "jest-prop-type-error"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!ol)"
    ]
  }
}
